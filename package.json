{
  "name": "codewing-ui",
  "version": "1.1.3",
  "description": "The UI for React Components",
  "author": {
    "name": "Abhishek Rijal",
    "email": "avee.ws@gmail.com"
  },
  "homepage": "https://codewing.co",
  "license": "MIT",
  "repository": "xqluzxwise/codewing-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "dependencies": {
    "@react-spring/web": "^9.4.4",
    "@wordpress/components": "^19.11.0",
    "@wordpress/i18n": "^4.9.0",
    "@wordpress/scripts": "^22.5.0",
    "array-move": "^2.1.0",
    "bezier-easing": "^2.1.0",
    "classnames": "^2.3.1",
    "customizer-sync-helpers": "^0.0.41",
    "deep-equal": "^2.0.5",
    "dompurify": "^2.3.0",
    "downshift": "^6.1.7",
    "lodash": "^4.17.21",
    "match-conditions": "^1.0.10",
    "md5": "^2.3.0",
    "nanoid": "^2.0.0",
    "react-select": "^5.3.1",
    "react-sortable-hoc": "^1.8.3",
    "react-spring": "8.0.1",
    "react-window": "^1.8.7",
    "simple-linear-scale": "^1.0.1",
    "throttle-debounce": "^5.0.0",
    "underscore": "^1.13.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^18.1.0",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ]
}